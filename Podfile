# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

target 'QNPaySDK' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for QNPaySDK
  pod 'Coastline'
  pod 'LZRequest'

  project 'QNPaySDK'

end


target 'PayTest' do
	  use_frameworks!
	
	pod 'Coastline'
	project 'PayTest/PayTest'
end


workspace 'QNPaySDK'
